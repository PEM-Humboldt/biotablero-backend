{
  "extends": [
    "../../.eslintrc",
    "airbnb-typescript/base",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint/eslint-plugin",
    "eslint-plugin-tsdoc"
  ],
  "rules": {
    "tsdoc/syntax": "warn",
    "@typescript-eslint/array-type": ["error", "generic"],
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/explicit-function-return-type" : 0
  }
}
